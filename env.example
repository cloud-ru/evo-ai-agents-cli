# AI Agents CLI Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# IAM Authentication
# =============================================================================
# IAM credentials for authentication
IAM_KEY_ID=your-iam-key-id
IAM_SECRET=your-iam-secret
IAM_ENDPOINT=https://iam.api.cloud.ru

# =============================================================================
# Project Configuration
# =============================================================================
# Project ID for AI Agents
PROJECT_ID=your-project-id

# =============================================================================
# API Configuration
# =============================================================================
# AI Agents API endpoint
PUBLIC_API_ENDPOINT=ai-agents.api.cloud.ru

# =============================================================================
# Service Configuration
# =============================================================================
# Application environment (dev, stage, prod)
SERVICE_APP_ENVIRONMENT=dev

# Service name
SERVICE_NAME=ai-agents-cli

# Log level (debug, info, warn, error)
SERVICE_LOG_LEVEL=debug

# Service version
SERVICE_VERSION=1.0.0

# =============================================================================
# IAM Service Account Configuration
# =============================================================================
# IAM BFF host
IAM_BFF_HOST=your-bff-host

# IAM client credentials
IAM_CLIENT_ID=your-iam-client-id
IAM_CLIENT_SECRET=your-iam-client-secret

# =============================================================================
# Performance Configuration
# =============================================================================
# Bulk operations concurrency factor
BULK_OPERATIONS_CONCURRENCY=20
